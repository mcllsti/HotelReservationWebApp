@model Testing_IdentityDatabase_Inital_30_03.Models.ExpandedUserDTO
@{
    ViewBag.Title = "LockoutUser";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br/>
<br />
<br />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
<div class="form-horizontal">
    <h4>Lockout User</h4>
    @Html.ValidationSummary(true,
           "",
           new { @class = "text-danger" })
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(m => m.Email, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(m => m.UserName, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            @Html.DisplayFor(m => m.FirstName, new { @class = "form-control" })
            @Html.DisplayFor(m => m.LastName, new { @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.LockoutEndDate, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.EditorFor(m => m.LockoutEndDate, new { @class = "form-control" })
        </div>
    </div>






    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Lockout" class="btn btn-default" />
            @Html.ActionLink("Back to Edit User",
            "EditUser",
            new { UserName = Model.UserName },
            new { @class = "btn btn-default" })
        </div>
    </div>
</div>
}














